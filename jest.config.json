{
    "setupFiles": [
        "raf/polyfill",
        "<rootDir>/src/tests/setupTest.js"
    ],
    "testMatch": [
        "<rootDir>/src/tests/**/*.test.js",
        "<rootDir>/src/tests/**/*.test.jsx"
    ],
    "snapshotSerializers": [
        "enzyme-to-json/serializer"
    ],
    "verbose": true
}